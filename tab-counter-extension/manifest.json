{
  "manifest_version": 3,
  "name": "Tab Counter",
  "version": "1.0",
  "description": "Counts tabs in the current window and displays their URLs",
  "permissions": [
    "tabs",
    "downloads"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Tab Counter"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}